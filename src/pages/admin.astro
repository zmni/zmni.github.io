<!doctype html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"
    />
    <meta name="robots" content="noindex" />
    <link href="/admin/config.yml" type="application/yaml" rel="cms-config-url" />
    <title>Bintang Surya Madani</title>
  </head>
  <body>
    <script src="https://unpkg.com/@sveltia/cms/dist/sveltia-cms.js" is:inline></script>
    <script is:inline>
      const observer = new MutationObserver(() => {
        const poweredBy = document.querySelector(".powered-by")
        if (poweredBy) {
          poweredBy.remove()
          observer.disconnect()
        }
      })
      observer.observe(document.body, {
        childList: true,
        subtree: true,
      })
    </script>
  </body>
</html>
